{
    "name": "datapp/router",
    "description": "simple PSR-7 regular expression router",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Dimmler",
            "email": "manuel.dimmler@datapp.de"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Datapp\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datapp\\Router\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3",
        "nyholm/psr7": "^1.4",
        "squizlabs/php_codesniffer": "*"
    }
}
